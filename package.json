{
  "name": "restyle.css",
  "description": "Yet another default styles reset library.",
  "version": "1.0.8",
  "main": "./dist/index.js",
  "restyle.css/bundle": "./dist/bundle.js",
  "restyle.css/margins": "./dist/margins.js",
  "restyle.css/scrollbar": "./dist/scrollbar.js",
  "restyle.css/checkboxes": "./dist/checkboxes.js",
  "license": "ISC",
  "homepage": "https://github.com/casadelaweb/restyle.css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casadelaweb/restyle.css.git"
  },
  "author": "https://github.com/casadelaweb/",
  "scripts": {
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "open": "start ./index.html",
    "watch": "npm run webpack watch --node-env=development --progress",
    "watch-open": "npm run dev && npm run open && npm run watch",
    "dev": "webpack --node-env=development --progress",
    "prepare": "node prepare.mjs",
    "build": "webpack --node-env=production --progress && npm run prepare"
  },
  "devDependencies": {
    "css-loader": "^6.7.1",
    "eslint": "^8.28.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  }
}
